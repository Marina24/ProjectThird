<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.projectthird">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".ui.MainApplication">

        <activity android:name=".ui.StandardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.SingleTopActivity" android:launchMode="singleTop" />
        <activity android:name=".ui.SingleTaskActivity" android:launchMode="singleTask" />
        <activity android:name=".ui.SingleInstanceActivity" android:launchMode="singleInstance"/>
    </application>

    <uses-permission android:name="android.permission.GET_TASKS"/>

</manifest>
